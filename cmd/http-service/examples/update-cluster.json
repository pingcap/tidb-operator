{
    "cluster_id":"tidb-cluster-123",
    "version":"v7.1.1",
    "pd":{
       "replicas":1,
       "resource":{
          "cpu":1,
          "memory":2,
          "storage":1
       },
       "config":{
          "schedule.patrol-region-interval":"100ms"
       }
    },
    "tikv":{
       "replicas":1,
       "resource":{
          "cpu":1,
          "memory":4,
          "storage":10
       },
       "config":{
          "server.simplify-metrics":false
       }
    },
    "tiflash":{
       "replicas":1,
       "resource":{
          "cpu":1,
          "memory":4,
          "storage":5
       },
       "config":{
          "profiles.default.max_memory_usage_for_all_queries":0.8
       },
       "learner_config":{
          "security.redact-info-log":false
       }
    },
    "tidb":{
       "replicas":1,
       "resource":{
          "cpu":1,
          "memory":4,
          "storage":0
       },
       "config":{
          "binlog.enable":false,
          "log.file.max-backups":3,
          "log.slow-threshold":300
       }
    }
}
